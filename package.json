{
  "name": "react-native-todos",
  "version": "0.0.1",
  "description": "Todos with ReactNative + TypeScript",
  "author": "GyeongMin Lee",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "eject": "rm -rf android/ ios/; react-native eject",
    "link": "react-native link; react-native link react-native-svg; app-icon generate",
    "start": "react-native start",
    "android": "emulator -avd $(emulator -list-avds | head -n 1) &>/dev/null & react-native run-android",
    "ios": "react-native run-ios",
    "xcode": "open ./ios/Todos.xcodeproj/"
  },
  "dependencies": {
    "react": "^16.6.3",
    "react-native": "^0.57.7",
    "react-native-svg": "^8.0.9",
    "react-native-svg-uri": "^1.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@types/react": "^16.7.13",
    "@types/react-native": "^0.57.17",
    "app-icon": "^0.6.3",
    "metro-react-native-babel-preset": "^0.50.0",
    "react-native-typescript-transformer": "^1.2.10",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.1"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  }
}
